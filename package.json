{
  "name": "jk-test",
  "displayName": "jk-test",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onChatParticipant:jk-test.jk-agent"
  ],
  "main": "./out/extension.js",
  "contributes":{
    "chatParticipants": [
    {
        "id": "jk-test.jk-agent",
        "fullName": "JK Test QA Agent",
        "name": "JKAgent",
        "description": "Let's check your code.",
        "isSticky": true,
        "commands": [
          {
            "name": "vulnerabilities",
            "description": "Analyze the code for vulnerabilities."
          },
          {
            "name": "oversights",
            "description": "Analyze the code for oversights or edge cases."
          }
        ]
    }
    ]
}
,
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  },
  "extensionDependencies": [
	  "vscode.git"
  ]
}
